.container{
  padding: 0;
  width: 100%;
  display: flex;
  background: #F0F0F3;
}
.topBtn{
  width: 100%;
  height: 50px;
  white-space: nowrap;
  background: white;
}
.btnview{
  width: 33.33%;
  height: 100%;
  display: inline-block;
  
}
.subBtn{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.line{
  width: 50%;
  height: 1px;
  margin-top: 10px;
}
.subBtn label{
  font-size: 14px;
  text-align: center;
  /* color: #506473; */
}
.tab{
  display: flex;
  flex-direction: column;
  width: 100%;
  background: #F0F0F3;
}
.tr{
  width: 100%;
  height: 40px;
  display: inline-block;

  border-bottom: 0.5px solid #F0F0F3;
}
.td{
  margin-left: 15px;
  margin-right: 15px;
  border-radius: 5px;
  margin-top: 10px;
  height: 100px;
  background: white;
  display: flex;
  flex-direction: row;
}
.left{
  height: 100%;
  width: 100px;
  background: #32AFFF;
  display: flex;
  flex-direction: column; 
  justify-content: center;
  border-top-left-radius:5px; 
  border-bottom-left-radius:5px; 
}
.left label{
  color: white;
  font-size: 12px;
  text-align: center;
}
.td image{
  height: 100%;
  width: 15px;
}
.right{
  margin-left: 5px;
  display: flex;
  margin-right: 20px;
  flex-direction: column; 
  justify-content: center;
}
.right label{
  font-size: 12px;
  color: #506473;
}