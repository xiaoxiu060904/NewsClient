.container{
  display: flex;
  padding: 0;
  width: 100%;
  background: #F0F0F3;
  /* border: 1px solid red; */
}
.head{
  width: 100%;
  background-color: #EC4837;
}
.headtop{
  margin-top: 10px;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 40px;
}
.headtop label{
  color: white;
  font-size: 24px;
  opacity: 0.9;
}
.headtop image{
  margin-left: 5px;
  width: 25px;
  height: 25px;
}
.bg{
  position: absolute;
  width: 100%;
  height: 100px;
  /* z-index: -1; */
}
.bg image{
  width: 100%;
  height: 100%;
}
.headcenter{
  margin-top: 5px;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 30px;
}
.headcenter label{
  color: white;
  font-size: 11px;
  opacity: 0.6;
}
.headcenter image{
  margin-left: 5px;
  width: 10px;
  height: 10px;
}
.headbottom{
  width: 100%;
  height: 65px;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.headbottommodel{
  height: 100%;
  width: 40%; 
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.lab1{
  margin-top: 10px;
  font-size: 11px;
  color: white;
}
.lab2{
  margin-top: 3px;
  font-size: 14px;
  color: white;
}
.center{
  width: 100%;
}
.centertop{
  width: 100%;
  height: 40px;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.labView{
  width: 0.5px;
  height: 100%;
  background: white;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.labView label{
  height: 15px;
  width: 100%;
  background:#F0F0F3; 
}
.centertop button{
  width: 50%;
  height: 100%;
  background: white;
  color: #EC4837;
  font-size: 15px;
  border-radius: 0;
}
.centertop button::after{
  border:none; 
}
.centerbottom{
  width: 100%;
  height: 80px;
  margin-top: 10px;
  background: white;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.activemodel{
  margin-top: 0.5px;
  background: white;
  display: flex;
  width: 33.33%;
  height: 70px;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.activemodel image{
  width: 30%;
  height: 30%;
}
.activemodel text{
  margin-top: 10px;
  font-size: 12px;
  color: #506473;
}
.bottom{
  width: 100%;
}
.table {
  display: flex;
  flex-direction: column;
  width: 100%;
  background-color: #F0F0F3;
}
.tr {
  width: 100%;
  height: 40px;
  background: white;
  margin-bottom: 0.5px;
}
.tr1 {
  margin-top: 10px;
  width: 100%;
  height: 40px;
  background: white;
  margin-bottom: 0.5px;
}
.td {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  /* justify-content: center; */
}
.tdleft{
  width: 50%;
  height: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.tdleft image{
  margin-left: 15px;
  width: 20px;
  height: 15px;
}
.tdleft text{
  margin-left: 10px;
  font-size: 12px;
  color: #506473;
}

.tdright{
  width: 50%;
  height: 100%;
  display: flex;
  flex-direction: row-reverse;
  align-items: center;
}
.tdright text{
  margin-right:10px;
  font-size: 12px;
  color: #506473;
}
.tdright image{
  margin-right:15px ;
  width: 12px;
  height: 12px;
}
.aler_view{
  width: 100%;  
  height: 100%;  
  position: fixed;  
  top: 0;  
  left: 0;  
  z-index: 1000;  
  /* background: #000;  
  opacity: 0.8;   */
}
.bgView{
  width: 100%;  
  height: 100%;  
  position: fixed; 
  background: #000;  
  opacity: 0.8;  
  z-index: -1; 
}
.sub{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  
}
.sub image{
  width: 258px;
  height: 214px;
}
.sub button{
  width: 115px;
  height: 31px;
  margin-top: 20px;
  background:  #EC4837;
  color: white;
  font-size: 14px;
}


