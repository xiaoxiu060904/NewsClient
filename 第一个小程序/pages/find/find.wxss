.container{
  display: flex;
  flex-direction: column;
  padding: 0;
  background: #F0F0F3;
}
.topView{
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 80px;
  border: 0.5px solid red;
}
.centerView{
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 160px;
  flex-wrap: wrap;
}



.table {
  border: 0px solid darkgray;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  background: #F0F0F3;
  width: 100%;
}
.tr {
  width: 33.33%;
  height: 80px;
  justify-content: center;
  align-items: center;
  background: white;
}
.tr1 {
  margin-top: 10px;
  width: 33.33%;
  height: 80px;
  justify-content: center;
  align-items: center;
  background: white;
}
.td {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
   
}
.td image{
  width: 30%;
  height: 30%;
}
.td text{
  margin-top: 10px;
  font-size: 12px;
  color: #506473;
}


